                                                      Min="{Binding Temperature_Min, Mode=OneWay}"
                                                      Text="{Binding TemperatureSetpoint_3, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="6"
                                Grid.Column="1"
                                BorderThickness="0,0,1,1"
