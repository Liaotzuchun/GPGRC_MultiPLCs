                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="7"
                                Grid.Column="1"
                                BorderThickness="0,0,1,1"
                                IsEnabled="{Binding IsEnabled, ElementName=Border5, Mode=OneWay}"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding Temperature_Max, Mode=OneWay}"
                                               Minimum="{Binding Temperature_Min, Mode=OneWay}"
                                               Value="{Binding TemperatureSetpoint_5, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      Max="{Binding Temperature_Max, Mode=OneWay}"
                                                      Min="{Binding Temperature_Min, Mode=OneWay}"
                                                      Text="{Binding TemperatureSetpoint_5, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="8"
                                Grid.Column="1"
                                BorderThickness="0,0,1,1"
                                IsEnabled="{Binding IsEnabled, ElementName=Border6, Mode=OneWay}"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding Temperature_Max, Mode=OneWay}"
                                               Minimum="{Binding Temperature_Min, Mode=OneWay}"
                                               Value="{Binding TemperatureSetpoint_6, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      Max="{Binding Temperature_Max, Mode=OneWay}"
                                                      Min="{Binding Temperature_Min, Mode=OneWay}"
                                                      Text="{Binding TemperatureSetpoint_6, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="3"
                                Grid.Column="2"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding RampTime_Max, Mode=OneWay}"
                                               Minimum="{Binding RampTime_Min, Mode=OneWay}"
                                               Value="{Binding RampTime_1, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
