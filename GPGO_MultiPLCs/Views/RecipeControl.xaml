<UserControl x:Class="GPGO_MultiPLCs.Views.RecipeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:local="clr-namespace:GPGO_MultiPLCs.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:models="clr-namespace:GPGO_MultiPLCs.Models"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:views="clr-namespace:GPMVVM.Views;assembly=GPMVVM"
             xmlns:vm="clr-namespace:GPGO_MultiPLCs.ViewModels"
             d:DataContext="{d:DesignInstance vm:RecipeControl_ViewModel}"
             d:DesignHeight="600"
             d:DesignWidth="1024"
             Foreground="{StaticResource BaseForeground}"
             Loaded="UserControl_Loaded"
             mc:Ignorable="d">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.4*" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid Margin="0,0,4,0"
              Background="{StaticResource WindowBackgroundBrush6}"
              IsEnabled="{Binding User.Level, Source={StaticResource Mediator}, Converter={StaticResource IsMoreThanValue}, ConverterParameter=2, Mode=OneWay}">
            <TabControl x:Name="TC"
                        views:TabControl.EnableSliding="True">
                <TabItem Height="44"
                         BorderThickness="1,1,1,0"
                         FontSize="16"
                         Header="{DynamicResource 目前配方}">
                    <Grid Background="{StaticResource WindowBackgroundBrush6}"
                          ClipToBounds="True"
                          DataContext="{Binding SelectedRecipe, Mode=OneWay}"
                          IsEnabled="{Binding Converter={StaticResource IsNotNull}, Mode=OneWay}"
                          TextElement.FontSize="14"
                          TextElement.FontWeight="Bold">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="80" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <ToggleButton Grid.Row="0"
                                      Grid.Column="0"
                                      Grid.ColumnSpan="2"
                                      Content="{DynamicResource 氮氣模式}"
                                      FontSize="18"
                                      IsChecked="{Binding NitrogenMode, Mode=TwoWay}"
                                      Style="{StaticResource ToggleSwitch}" />
                        <Border Grid.Row="0"
                                Grid.Column="2"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,1,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 充氣逾時}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="0"
                                Grid.Column="3"
                                BorderThickness="0,1,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding InflatingTime_Max, Mode=OneWay}"
                                               Minimum="{Binding InflatingTime_Min, Mode=OneWay}"
                                               Value="{Binding InflatingTime, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      IntOnly="True"
                                                      Max="{Binding InflatingTime_Max, Mode=OneWay}"
                                                      Min="{Binding InflatingTime_Min, Mode=OneWay}"
                                                      Text="{Binding InflatingTime, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="0"
                                Grid.Column="4"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,1,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 含氧量}" />
                                <Run Text="(%)" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="0"
                                Grid.Column="5"
                                BorderThickness="0,1,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="21"
                                               Minimum="0.1"
                                               Value="{Binding OxygenContentSet, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      Max="21"
                                                      Min="0.1"
                                                      Text="{Binding OxygenContentSet, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="1"
                                Grid.Column="0"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="1,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Text="{DynamicResource 使用段數}" />
                        </Border>
                        <Border Grid.Row="1"
                                Grid.Column="1"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding SegmentCounts_Max, Mode=OneWay}"
                                               Minimum="{Binding SegmentCounts_Min, Mode=OneWay}"
                                               Value="{Binding SegmentCounts, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      IntOnly="True"
                                                      Max="{Binding SegmentCounts_Max, Mode=OneWay}"
                                                      Min="{Binding SegmentCounts_Min, Mode=OneWay}"
                                                      Text="{Binding SegmentCounts, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="1"
                                Grid.Column="2"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 降溫溫度}" />
                                <Run Text="(°C)" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="1"
                                Grid.Column="3"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding CoolingTemperature_Max, Mode=OneWay}"
                                               Minimum="{Binding CoolingTemperature_Min, Mode=OneWay}"
                                               Value="{Binding CoolingTemperature, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      Max="{Binding CoolingTemperature_Max, Mode=OneWay}"
                                                      Min="{Binding CoolingTemperature_Min, Mode=OneWay}"
                                                      Text="{Binding CoolingTemperature, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="1"
                                Grid.Column="4"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 降溫時間}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="1"
                                Grid.Column="5"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding CoolingTime_Max, Mode=OneWay}"
                                               Minimum="{Binding CoolingTime_Min, Mode=OneWay}"
                                               Value="{Binding CoolingTime, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      Max="{Binding CoolingTime_Max, Mode=OneWay}"
                                                      Min="{Binding CoolingTime_Min, Mode=OneWay}"
                                                      Text="{Binding CoolingTime, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="0"
                                Grid.RowSpan="2"
                                Grid.Column="6"
                                Background="Transparent"
                                BorderThickness="0,1,1,1"
                                Style="{StaticResource ValueBorder}" />
                        <Border Grid.Row="2"
                                Grid.Column="0"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="1,0,1,1"
                                Style="{StaticResource ValueBorder}" />
                        <Border Grid.Row="2"
                                Grid.Column="1"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 目標溫度}" />
                                <Run Text="(°C)" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="2"
                                Grid.Column="2"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 升溫時間}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="2"
                                Grid.Column="3"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 升溫警報}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="2"
                                Grid.Column="4"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 恆溫時間}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="2"
                                Grid.Column="5"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 恆溫警報}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="2"
                                Grid.Column="6"
                                Background="{StaticResource WindowBackgroundBrush2}"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center">
                                <Run Text="{DynamicResource 恆溫補償時間}" />
                                <Run Text="(" /><Run Text="{DynamicResource 分}" /><Run Text=")" />
                            </TextBlock>
                        </Border>
                        <Border Grid.Row="3"
                                Grid.Column="0"
                                BorderThickness="1,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Text="1" />
                        </Border>
                        <Border x:Name="Border2"
                                Grid.Row="4"
                                Grid.Column="0"
                                BorderThickness="1,0,1,1"
                                IsEnabled="{Binding SegmentCounts, Mode=OneWay, Converter={StaticResource IsMoreThanValue}, ConverterParameter=2}"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Text="2" />
                        </Border>
                        <Border x:Name="Border3"
                                Grid.Row="5"
                                Grid.Column="0"
                                BorderThickness="1,0,1,1"
                                IsEnabled="{Binding SegmentCounts, Mode=OneWay, Converter={StaticResource IsMoreThanValue}, ConverterParameter=3}"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Text="3" />
                        </Border>
                        <Border x:Name="Border4"
                                Grid.Row="6"
                                Grid.Column="0"
                                BorderThickness="1,0,1,1"
                                IsEnabled="{Binding SegmentCounts, Mode=OneWay, Converter={StaticResource IsMoreThanValue}, ConverterParameter=4}"
                                Style="{StaticResource ValueBorder}">
                            <TextBlock HorizontalAlignment="Center"
