                            <TextBlock HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       Text="6" />
                        </Border>
                        <Border Grid.Row="3"
                                Grid.Column="1"
                                BorderThickness="0,0,1,1"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding Temperature_Max, Mode=OneWay}"
                                               Minimum="{Binding Temperature_Min, Mode=OneWay}"
                                               Value="{Binding TemperatureSetpoint_1, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
                                                      Max="{Binding Temperature_Max, Mode=OneWay}"
                                                      Min="{Binding Temperature_Min, Mode=OneWay}"
                                                      Text="{Binding TemperatureSetpoint_1, Mode=TwoWay}" />
                            </views:SpinControl>
                        </Border>
                        <Border Grid.Row="4"
                                Grid.Column="1"
                                BorderThickness="0,0,1,1"
                                IsEnabled="{Binding IsEnabled, ElementName=Border2, Mode=OneWay}"
                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding Temperature_Max, Mode=OneWay}"
                                               Minimum="{Binding Temperature_Min, Mode=OneWay}"
                                               Value="{Binding TemperatureSetpoint_2, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
                                </views:SpinControl.SmallChange>
                                <views:NumericTextBox BorderThickness="0"
