                                Style="{StaticResource ValueBorder}">
                            <views:SpinControl Maximum="{Binding RampTime_Max, Mode=OneWay}"
                                               Minimum="{Binding RampTime_Min, Mode=OneWay}"
                                               Value="{Binding RampTime_2, Mode=TwoWay}">
                                <views:SpinControl.SmallChange>
                                    <system:Double>0.1</system:Double>
